<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageId>BigNumberRecursiveMod</PackageId>
		<Version>1.0.1</Version>
		<Authors>Valadis Pitsas</Authors>
		<Company></Company>
		<Description>Recursively calculates modulo for large numbers represented as strings.</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/cpitsas/BigNumberRecursiveMod</RepositoryUrl>
		<PackageProjectUrl>https://github.com/cpitsas/BigNumberRecursiveMod</PackageProjectUrl>
		<RepositoryType>git</RepositoryType>
	</PropertyGroup>

	<ItemGroup>
		<None Include="README.md" Pack="true" PackagePath="" />
	</ItemGroup>

</Project>
